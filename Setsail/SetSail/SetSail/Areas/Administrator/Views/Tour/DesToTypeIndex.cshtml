@model IEnumerable<SetSail.Models.DesToType>

@{
    ViewBag.Title = "DesToTypeIndex";
    Layout = "~/Areas/Administrator/Views/Shared/_Layout.cshtml";
}

<h2>Destination with Tour Type</h2>

<p>
    @Html.ActionLink("Create New", "DesToTypeCreate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Destination.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TourType.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Destination.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TourType.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "DesToTypeUpdate", new { id=item.Id }) |
            <a href="@Url.Action("DesToTypeDelete","Tour", new { id = item.Id })" onclick="return confirm('Are you sure?')">Delete</a>
        </td>
    </tr>
}

</table>
