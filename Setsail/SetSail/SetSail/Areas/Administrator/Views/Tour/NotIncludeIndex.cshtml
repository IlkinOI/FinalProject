@model IEnumerable<SetSail.Models.NotInclude>

@{
    ViewBag.Title = "NotIncludeIndex";
    Layout = "~/Areas/Administrator/Views/Shared/_Layout.cshtml";
}

<h2>Tour Not Included Features</h2>

<p>
    @Html.ActionLink("Create New", "NotIncludeCreate")
</p>
<table class="table table-responsive">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Feature)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tour.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Feature)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tour.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "NotIncludeUpdate", new { id=item.Id }) |
            <a href="@Url.Action("NotIncludeDelete","Tour", new { id = item.Id })" onclick="return confirm('Are you sure?')">Delete</a>
        </td>
    </tr>
}

</table>
